{
  "compilerOptions": {
    "baseUrl": ".",
    "checkJs": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "esnext",
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".eslintrc.cjs",
    "**/*.astro",
    "**/*.cjs",
    "**/*.js",
    "**/*.mjs",
    "**/*.ts",
    "**/*.tsx"
  ]
}
